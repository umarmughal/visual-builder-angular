{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      [
        "app",
        "cui",
        "layout",
        "air",
        "kit",
        "vb"
      ],
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      [
        "app",
        "cui",
        "layout",
        "air",
        "kit",
        "vb"
      ],
      "kebab-case"
    ]
  }
}